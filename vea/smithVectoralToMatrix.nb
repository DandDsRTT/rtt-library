smithVectoralToMatrix[{w_, s_}] := Module[{g, d, t, flat, f, h},
  g = Abs[s];

  d = veaDimensionality[{w, s}];

  t = vectoralToTensor[{w, d, g}];
  f = Flatten[t, g - 1];


  (* flat = ArrayFlatten[t] *)


  Print["g: ", g, " w: ", w, " t: ", t // MatrixForm, " f: ", f // MatrixForm];

  h = hnf[t];
  Print["h: ", h];


