smithVectoralToMatrix[{w_, g_}] := Module[{absG, d, t, flat, f, h},
  absG = Abs[g];

  d = veaDimensionality[{w, g}];

  t = vectoralToTensor[{w, d, absG}];
  f = Flatten[t, absG - 1];


  (* flat = ArrayFlatten[t] *)


  Print["absG: ", absG, " w: ", w, " t: ", t // MatrixForm, " f: ", f // MatrixForm];

  h = hnf[t];
  Print["h: ", h];


