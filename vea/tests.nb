Print["(* conversion between matrix *)"];

tester[{w_, s_}, a_, debug_ : False] := Module[{convertedM, convertedV},
  convertedA = If[
    s == 0,
    vectoralToMatrix[{w, s}, dimensionality[a]],
    vectoralToMatrix[{w, s}]
  ];
  convertedV = If[
    isSquare[a] && s > 0, (* because otherwise how could you tell that you want a multiCovector *)
    matrixToVectoral[a, True],
    matrixToVectoral[a]
  ];

  If[debug, Print["convertedA: ", convertedA, " a: ", a, " convertedV: ", convertedV, " v: ", {w, s}], ""];

  convertedA == a && convertedV == {w, s}
];

