Do[
  d = RandomInteger[{1, 5}];
  r = RandomInteger[{1, d}];
  m = RandomInteger[{-9, 9}, {r, d}];

  If[AllTrue[m, # == 0&, 2], Continue[]]; (* skip if generated an entirely zeros mapping *)
  If[RandomInteger[] == 1, m = Transpose[m]]; (* for checking comma-bases too *)
  Print[m];

  v = matrixToVectoral[m];
  m2 = vectoralToMatrix[v];

  If[canonicalForm[m2] != canonicalForm[m], Print["BAD BAD BAD! mapping: ", m, " computed equiv vectoral: ", v, " and then back to matrix: ", m2]],
  100
]
Print["done"]
