Print["dual"];

dualTester[m_, c_] := dual[m] == canonicalForm[c] && dual[c] == canonicalForm[m];

dualTester[{{1,0,0},{0,1,0},{0,0,1}}, 1]

dualTester[{{0}}, {{1}}]
dualTester[{{0,0}}, {{1, 0}, {0,1}}]
dualTester[{{0},{0}}, {{1,0},{0,1}}]

dualTester[{{12,19}}, {{-19},{12}}]
